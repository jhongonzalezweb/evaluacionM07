<%- include("template/cabecera", {tituloWeb: 'Página Crear Nuevo Cliente' }) %>

    <div class="container">

        <h1>Ingresa la mascota</h1>

        <form action="agregarMascota" class="my-2" method="POST">

            <input type="text" placeholder="Ingrese Nombre de la Mascota" class="form-control my-2" name="nombre">

            <input type="text" placeholder="Ingrese Raza" class="form-control my-2" name="raza">

            <input type="text" placeholder="Fecha Nacimiento Mascota" class="form-control my-2" name="fecha_nacimiento">

            <input type="text" placeholder="Ingrese Dueño" class="form-control my-2" name="nombre_dueno">

            <button type="submit" class="btn btn-dark btn-block">
                Agregar Mascota
            </button>

        </form>

    </div>

    <%- include("template/footer") %>